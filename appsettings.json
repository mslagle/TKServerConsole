{
  "Teamkist": {
    "ServerIP": "0.0.0.0",
    "ServerPort": 50000,
    "LevelName": "TeamkistServer",
    "AutoSaveInterval": 60,
    "BackupCount": 5,
    "LoadBackupOnStart": true,
    "KeepBackupWithNoEditors": true
  },
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
    },
    "File": {
      "IncludeScopes": true,
      "BasePath": "Logs",
      "MaxFileSize": 1024,
      "Files": [
        {
          "Path": "teamkist.log"
        }
      ]
    }
  }
}
